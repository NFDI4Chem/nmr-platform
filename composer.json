{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andrewdwallo/filament-companies": "^4.2",
        "archilex/filament-filter-sets": "*",
        "awcodes/filament-table-repeater": "^3.1",
        "bezhansalleh/filament-exceptions": "^2.1",
        "bezhansalleh/filament-panel-switch": "^1.0",
        "bezhansalleh/filament-shield": "^3.3",
        "brnysn/laravel-world": "^2.0",
        "filament/filament": "^3.3",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.31",
        "laravel/jetstream": "^5.3",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.20",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.0",
        "marcogermani87/filament-cookie-consent": "^1.1",
        "ralphjsmit/laravel-filament-media-library": "^3.9",
        "spatie/laravel-cookie-consent": "^3.3",
        "stechstudio/filament-impersonate": "^3.16",
        "stephenjude/filament-debugger": "^4.0",
        "tapp/filament-auditing": "^3.0"
    },
    "require-dev": {
        "doctrine/dbal": "^4.2",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.43",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
          "type": "composer",
          "url": "https://filament-filter-sets.composer.sh"
        },
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ]
}
